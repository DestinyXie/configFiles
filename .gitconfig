[user]
    name = DestinyXie
    email = xber1986@gmail.com
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    oneline = log --pretty=oneline
[http]
	postBuffer = 52428800000
[color]
    branch = auto
    diff = auto
    status = auto
    grep = auto
    ui = auto
    interactive = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[credential]
    helper = cache --timeout 36000
[core]
    editor = vim
	autocrlf = input
	whitespace = cr-at-eol
[pack]
    threads = 1
    deltaCacheSize = 128m
    windowMemory = 50m
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[url "https://"]
	insteadOf = git://
